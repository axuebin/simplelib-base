  "bugs": {
    "url": "https://github.com/axuebin/simplelib-base/issues"
  },
  "homepage": "https://github.com/axuebin/simplelib-base#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "rollup": "^1.16.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.0",
    "typescript": "^3.5.2"
  }
